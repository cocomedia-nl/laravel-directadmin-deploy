{
	"name": "cocomedia-nl/laravel-directadmin-deploy",
	"description": "Laravel package for automated DirectAdmin deployment with GitHub Actions support",
	"type": "library",
	"version": "0.1.3",
	"license": "MIT",
	"keywords": [
		"laravel",
		"directadmin",
		"deployment",
		"github-actions",
		"ssh"
	],
	"authors": [
		{
			"name": "Cocomedia",
			"email": "info@cocomedia.nl"
		}
	],
	"require": {
		"php": "^8.2",
		"illuminate/support": "^11.0|^12.0",
		"illuminate/console": "^11.0|^12.0",
		"illuminate/process": "^11.0|^12.0",
		"symfony/console": "^7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.0",
		"orchestra/testbench": "^8.0|^9.0"
	},
	"autoload": {
		"psr-4": {
			"CocomediaNL\\LaravelDirectAdminDeploy\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CocomediaNL\\LaravelDirectAdminDeploy\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"CocomediaNL\\LaravelDirectAdminDeploy\\DirectAdminDeployServiceProvider"
			]
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	}
}
